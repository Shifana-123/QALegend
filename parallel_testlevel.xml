<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests" thread-count="2">
    <test name="Smoke Test">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="org.qalegend.test.AddUsersPageTest"/>
        </classes>
    </test>
    <test name="Regression Test">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="org.qalegend.test.HomePageTest"/>
            <class name="org.qalegend.test.LoginPageTest"/>
        </classes>
    </test>
    <test name="Sanity Test">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="org.qalegend.test.ResetPageTest"/>
            <class name="org.qalegend.test.UserManagementPageTest"/>
        </classes>
    </test>
</suite>